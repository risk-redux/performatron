<!DOCTYPE html>
<html lang="en">
  <%= render partial: "shared/head" %>
  <body>

    <!-- Wrap all page content here -->
    <div id="wrap">
      <%= render partial: "shared/nav" %>

      <!-- Begin page content -->
      <div class="container">
        <div class="row">
          <div class="col-xs-2 sidebar">
            <%= yield(:sidebar) %>
          </div>
          <div class="col-xs-10 col-xs-offset-2 main">
            <%= yield(:main) %>
          </div>
        </div>
      </div>
    </div>
    <!-- Close up the page wrapping. -->

  </body>
</html>
